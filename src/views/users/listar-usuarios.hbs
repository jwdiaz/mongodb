<div class="container">

    <h2>Usuarios </h2>
        <hr />

        <div class="row">
            <div class="col-md-12 mx-auto">
                <table class="table">
                    <thead>
                        <tr>
                            <th scope="col">Documento</th>
                            <th scope="col">Nombre</th>
                            <th scope="col">Correo</th>
                            <th scope="col">Telefono</th>
                            <th scope="col">Rol de usuario</th>

                        </tr>
                    </thead>
                    <tbody>
                        {{#each users}}
                        <tr>
                            <td scope="row">{{ documento}}</a></td>
                            <td>{{nombre}}</a></td>
                            <td>{{ correo }}</td>
                            <td>{{ telefono }}</td>
                            <th scope="row"><a href="/tipoUsuario/{{ _id }}">{{ tipo }}</a></th>
                            <th scope="row"><a href="/actualizarUsuario/{{ _id }}">Actualizar</a></th>

                        </tr>

                        {{/each}}
                    </tbody>
                </table>
                <hr />
            </div>

        </div>
</div>